        "test": "jest",
        "lint": "tslint -p tsconfig.json --fix",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^22.2.3",
        "@types/node": "^14.14.31",
        "codecov": "^3.1.0",
        "crypto-browserify": "^3.12.0",
        "jest": "^26.6.3",
        "prettier": "^1.13.2",
        "ts-jest": "^26.5.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.13.0",
        "typescript": "^4.1.5"
    },
    "jest": {
        "transform": {
            "^.+\\.(t|j)sx?$": "ts-jest"
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
