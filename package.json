        "PRNG"
    ],
    "homepage": "https://github.com/almasen/n-digit-token#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/almasen/n-digit-token"
    },
    "funding": "https://github.com/sponsors/almasen",
    "author": {
        "name": "Daniel Almasi",
        "email": "almd@pm.me",
        "url": "https://github.com/almasen"
    },
    "license": "MIT",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*"
    ],
    "scripts": {
